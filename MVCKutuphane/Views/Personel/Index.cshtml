@using MVCKutuphane.Models.Entity
@model List<TBLPERSONEL> 
@{
    ViewBag.Title = "Index";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h3>PERSONELLER</h3>
<table class="table table-striped">
    <tr style="background-color:rgb(252, 178, 22)">
        <th>ID</th>
        <th>PERSONEL</th>
        <th>SİL</th>
        <th>GÜNCELLE</th>

    </tr>
    @foreach (var pr in Model)
    {
        <tr>
            <th>@pr.ID</th>
            <td>@pr.PERSONEL</td>
            <td><a href="/Personel/PersonelSil/@pr.ID" class="btn btn-danger">SİL</a></td>
            <td><a href="/Personel/PersonelGetir/@pr.ID" class="btn btn-warning">GÜNCELLE</a></td>
        </tr>
    }
</table>
<a href="~/Personel/PersonelEkle" class="btn btn-success">Personel Ekle</a>

