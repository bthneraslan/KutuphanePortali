@using MVCKutuphane.Models.Entity
@model List<TBLHAREKET>
@{
    ViewBag.Title = "UyeKitapGecmis";
    Layout = "~/Views/Shared/Layout.cshtml";
}

<h2>@ViewBag.uye1</h2>

<table class="table table-bordered">
    <tr>
        <th>Kitap Adı</th>
        <th>Personel</th>
        <th>Alış Tarihi</th>
        <th>İade Tarihi</th>
    </tr>

    @foreach(var i in Model)
    {
        <tr>
            <td>@i.TBLKITAP.AD</td>
            <td>@i.TBLPERSONEL.PERSONEL</td>
            <td>@Convert.ToDateTime(i.ALISTARIH).ToString("dd/MM/yyyy")</td>
            <td>@Convert.ToDateTime(i.IADETARIH).ToString("dd/MM/yyyy")</td>
        </tr>
    }
</table>
